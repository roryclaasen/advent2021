{
  "name": "@roryclaasen/advent2021",
  "version": "2.5.0",
  "description": "My 2021 Advent of Code attempt",
  "bin": {
    "advent2021": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roryclaasen/advent2021.git"
  },
  "author": {
    "name": "Rory Claasen",
    "email": "contact@roryclaasen.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roryclaasen/advent2021/issues"
  },
  "homepage": "https://github.com/roryclaasen/advent2021#readme",
  "dependencies": {
    "@oclif/config": "^1.18.2",
    "@oclif/core": "^1.0.10",
    "@oclif/plugin-help": "^3.2.15",
    "cli-ux": "^6.0.6",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.9",
    "@oclif/test": "^2.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "chai": "^4.3.4",
    "conventional-changelog-eslint": "^3.0.9",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "advent2021",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "start": "node ./bin/run",
    "dev": "node ./bin/dev",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "build": "rimraf lib && tsc -b",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "npm run build && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=12"
  }
}
