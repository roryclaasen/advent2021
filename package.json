{
  "name": "@roryclaasen/advent2021",
  "version": "1.0.0",
  "description": "My 2021 Advent of Code attempt",
  "bin": {
    "advent2021": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roryclaasen/advent2021.git"
  },
  "author": {
    "name": "Rory Claasen",
    "email": "contact@roryclaasen.dev"
  },
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/roryclaasen/advent2021/issues"
  },
  "homepage": "https://github.com/roryclaasen/advent2021#readme",
  "dependencies": {
    "@oclif/command": "^1.8.6",
    "@oclif/config": "^1.18.1",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^3.2.9",
    "cli-ux": "^5.6.4",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@types/lodash": "^4.14.177",
    "conventional-changelog-eslint": "^3.0.9",
    "eslint": "^8.3.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "advent2021",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "start": "node ./bin/run",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "pnpm run lint",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@roryclaasen"
  }
}
